<?xml version="1.0"?>
<zcl:cluster xmlns:zcl="http://zigbee.org/zcl/clusters"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:type="http://zigbee.org/zcl/types"
  xmlns:xi="http://www.w3.org/2001/XInclude"
  xsi:schemaLocation="http://zigbee.org/zcl/clusters cluster.xsd http://zigbee.org/zcl/types type.xsd"
  id="040A" revision="1" name="ElectricalConductivity">
  <classification hierarchy="base" role="application" picsCode="EC" primaryTransaction="2" />
  <server>
    <attributes>
      <attribute id="0000" name="MeasuredValue" type="uint16" reportRequired="true" required="true" default="65535" >
        <restriction>
          <type:minInclusiveRef ref="MinMeasuredValue" />
          <type:maxInclusiveRef ref="MaxMeasuredValue" />
        </restriction>
       </attribute>
      <attribute id="0001" name="MinMeasuredValue" type="uint16" required="true" default="65535" >
        <restriction>
          <type:maxExclusiveRef ref="MaxMeasuredValue" />
        </restriction>
       </attribute>
      <attribute id="0002" name="MaxMeasuredValue" type="uint16" required="true" max="65534" default="65535" >
       <restriction>
          <type:minExclusiveRef ref="MinMeasuredValue" />
        </restriction>
       </attribute>
      <attribute id="0003" name="Tolerance" type="uint16" max="100" />
    </attributes>
  </server>
</zcl:cluster>

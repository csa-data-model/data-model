<?xml version="1.0"?>
<zcl:cluster xmlns:zcl="http://zigbee.org/zcl/clusters"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:type="http://zigbee.org/zcl/types"
  xmlns:xi="http://www.w3.org/2001/XInclude"
  xsi:schemaLocation="http://zigbee.org/zcl/clusters cluster.xsd http://zigbee.org/zcl/types type.xsd"
  id="0007" revision="0" name="On /Off Switch Configuration">
  <classification hierarchy="base" role="application" picsCode="OOSC" primaryTransaction="" />
  <server>
    <attributes>
      <attribute id="0000" name="SwitchType" type="enum8" required="true" default="-">
        <restriction>
          <type:enumeration value="00" name="Toggle" />
          <type:enumeration value="01" name="Momentary" />
          <type:enumeration value="02" name="Multifunction" />
        </restriction>
      </attribute>
      <attribute id="0010" name="SwitchActions" type="enum8" writable="true" required="true" default="0">
        <restriction>
          <type:enumeration value="00" name="On" />
          <type:enumeration value="01" name="Off" />
          <type:enumeration value="02" name="Toggle" />
        </restriction>
      </attribute>
    </attributes>
  </server>
</zcl:cluster>

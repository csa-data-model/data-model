<?xml version="1.0"?>
<zcl:cluster xmlns:schemaLocation="http://zigbee.org/zcl/clusters cluster.xsd http://zigbee.org/zcl/types type.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:type="http://zigbee.org/zcl/types" xmlns:xi="http://www.w3.org/2001/XInclude" id="0204" revision="0" name="Thermostat User Interface Configuration" xmlns:zcl="http://zigbee.org/zcl/clusters">
  <classification role="application" picsCode="TSUIC" />
  <server>
    <attributes>
      <attribute id="0000" name="TemperatureDisplayMode" type="enum8" writable="true" required="true" default="0x00 (Celsius)" />
      <attribute id="0001" name="KeypadLockout" type="enum8" writable="true" required="true" default="0x00 (no lockout)">
        <restriction>
          <type:enumeration value="00" name="NoLockout" />
          <type:enumeration value="01" name="Level1Lockout" />
          <type:enumeration value="02" name="Level2Lockout" />
          <type:enumeration value="03" name="Level3Lockout" />
          <type:enumeration value="04" name="Level4Lockout" />
          <type:enumeration value="05" name="Level5Lockout(leastFunctionalityAvailableToTheUser)" />
        </restriction>
      </attribute>
      <attribute id="0002" name="ScheduleProgrammingVisibility" type="enum8" writable="true" default="0">
        <restriction>
          <type:enumeration value="00" name="LocalScheduleProgrammingFunctionalityIsEnabledAtTheThermostat" />
          <type:enumeration value="01" name="LocalScheduleProgrammingFunctionalityIsDisabledAtTheThermostat" />
        </restriction>
      </attribute>
    </attributes>
  </server>
</zcl:cluster>
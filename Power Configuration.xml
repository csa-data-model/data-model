      <attribute id="0021" name="BatteryPercentageRemaining" type="uint8" max="255" default="0" />
      <attribute id="0030" name="BatteryManufacturer" type="string" writable="true" max="16" default="Empty string" />
      <attribute id="0031" name="BatterySize" type="enum8" writable="true" default="0xff">

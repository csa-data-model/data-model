<?xml version="1.0" encoding="utf-16"?>
<ZclCluster xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="0001" revision="0" name="Power Configuration">
  <classification role="utility" picsCode="PC" />
  <server>
    <attributes>
      <attribute id="0000" name="MainsVoltage" type="uint16" max="65535" default="-" />
      <attribute id="0001" name="MainsFrequency" type="uint8" max="255" default="-" />

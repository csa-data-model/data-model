          <facet xsi:type="enumerationType" value="18" name="BatteryVoltageThreshold2 or BatteryPercentageThreshold2 reached Battery Source 2" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="19" name="BatteryVoltageThreshold3 or BatteryPercentageThreshold3 reached Battery Source 2" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="32" name="BatteryVoltageMinThreshold or BatteryPercentageMinThreshold reached for Battery Source 3" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="33" name="BatteryVoltageThreshold1 or BatteryPercentageThreshold1 reached for Battery Source 3" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="34" name="BatteryVoltageThreshold2 or BatteryPercentageThreshold2 reached Battery Source 3" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="35" name="BatteryVoltageThreshold3 or BatteryPercentageThreshold3 reached Battery Source 3" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="48" name="Mains power supply lost/unavailable (i.e., device is running on battery)" xmlns="http://zigbee.org/zcl/types" />
        </restriction>
      </attribute>
    </attributes>
  </server>

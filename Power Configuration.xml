          <facet xsi:type="enumerationType" value="0" name="No battery " xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="1" name="Built in" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="2" name="Other" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="3" name="AA" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="4" name="AAA" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="5" name="C" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="6" name="D" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="7" name="CR2 (IEC: CR17355 / ANSI: 5046LC)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="8" name="CR123A (IEC: CR17345 / ANSI: 5018LC)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="255" name="Unknown" xmlns="http://zigbee.org/zcl/types" />
        </restriction>
      </attribute>
      <attribute id="0032" name="BatteryAHrRating" type="uint16" writable="true" max="65535" default="-" />
      <attribute id="0033" name="BatteryQuantity" type="uint8" writable="true" max="255" default="-" />
      <attribute id="0034" name="BatteryRatedVoltage" type="uint8" writable="true" max="255" default="-" />
      <attribute id="0035" name="BatteryAlarmMask" type="map8" writable="true" default="0000 0000">
        <restriction>
          <facet xsi:type="enumerationType" value="0" name="Battery voltage too low to continue operating the device’s radio (i.e., BatteryVoltageMinThreshold value has been reached)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="1" name="Battery Alarm 1 (i.e., Battery Voltage Threshold 1 or Battery Percentage Threshold 1 value has been reached)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="2" name="Battery Alarm 2 (i.e., Battery Voltage Threshold 2 or Battery Percentage Threshold 2 value has been reached)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="3" name="Battery Alarm 3 (i.e., Battery Voltage Threshold 3 or Battery Percentage Threshold 3 value has been reached)" xmlns="http://zigbee.org/zcl/types" />
        </restriction>
      </attribute>
      <attribute id="0036" name="BatteryVoltageMinThreshold" type="uint8" writable="true" max="255" default="0x0000" />
      <attribute id="0037" name="BatteryVoltageThreshold1" type="uint8" writable="true" max="255" default="0x00" />
      <attribute id="0038" name="BatteryVoltageThreshold2" type="uint8" writable="true" max="255" default="0x00" />
      <attribute id="0039" name="BatteryVoltageThreshold3" type="uint8" writable="true" max="255" default="0x00" />

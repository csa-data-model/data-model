          <facet xsi:type="enumerationType" value="0" name="BatteryVoltageMinThreshold or BatteryPercentageMinThreshold reached for Battery Source 1" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="1" name="BatteryVoltageThreshold1 or BatteryPercentageThreshold1 reached for Battery Source 1" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="2" name="BatteryVoltageThreshold2 or BatteryPercentageThreshold2 reached for Battery Source 1" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="3" name="BatteryVoltageThreshold3 or BatteryPercentageThreshold3 reached for Battery Source 1" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="16" name="BatteryVoltageMinThreshold or BatteryPercentageMinThreshold reached for Battery Source 2" xmlns="http://zigbee.org/zcl/types" />

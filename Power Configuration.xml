        <restriction>
          <facet xsi:type="enumerationType" value="0" name="Mains Voltage too low ( REF _Ref405912424 \r \h  \* MERGEFORMAT 3.3.2.2.2.2)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="1" name="Mains Voltage too high ( REF _Ref405912438 \r \h  \* MERGEFORMAT 3.3.2.2.2.3)" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="2" name="Mains power supply lost/unavailable (i.e., device is running on battery)" xmlns="http://zigbee.org/zcl/types" />
        </restriction>
      </attribute>
      <attribute id="0011" name="MainsVoltageMinThreshold" type="uint16" writable="true" max="65535" default="0x0000" />
      <attribute id="0012" name="MainsVoltageMaxThreshold" type="uint16" writable="true" max="65535" default="0xffff" />
      <attribute id="0013" name="MainsVoltageDwellTripPoint" type="uint16" writable="true" max="65535" default="0x0000" />

<?xml version="1.0" encoding="utf-16"?>
<ZclCluster xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" id="0008" revision="0" name="Level (this cluster specificiation)">
  <classification role="application" picsCode="LVL" />
  <server>
    <attributes>
      <attribute id="0000" name="CurrentLevel" type="uint8" required="true" default="0xff" />
      <attribute id="0001" name="RemainingTime" type="uint16" max="65535" default="0x0000" />
      <attribute id="0002" name="MinLevel" type="uint8" default="0x00" />
      <attribute id="0003" name="MaxLevel" type="uint8" min="255" default="0xff" />
      <attribute id="0004" name="CurrentFrequency" type="uint16" default="0x0000" />
      <attribute id="0005" name="MinFrequency" type="uint16" default="0x0000" />
      <attribute id="0006" name="MaxFrequency" type="uint16" min="65535" default="0x0000" />
      <attribute id="0010" name="OnOffTransitionTime" type="uint16" writable="true" max="65535" default="0x0000" />
      <attribute id="0011" name="OnLevel" type="uint8" writable="true" default="0xff" />
      <attribute id="0012" name="OnTransitionTime" type="uint16" writable="true" max="65534" default="0xffff" />
      <attribute id="0013" name="OffTransitionTime" type="uint16" writable="true" max="65534" default="0xffff" />
      <attribute id="0014" name="DefaultMoveRate" type="uint16" writable="true" max="254" default="MS" />
      <attribute id="000F" name="Options" type="map8" writable="true" default="0x00">
        <restriction>
          <facet xsi:type="enumerationType" value="0" name="ExecuteIfOff" xmlns="http://zigbee.org/zcl/types" />
          <facet xsi:type="enumerationType" value="1" name="Reserved for Derived Clusters" xmlns="http://zigbee.org/zcl/types" />
        </restriction>
      </attribute>
      <attribute id="4000" name="StartUpCurrentLevel" type="uint8" writable="true" max="255" default="MS" />
    </attributes>
    <commands>
      <command id="00" name="Move to Level " required="true">
        <fields>
          <field name="Level" type="uint8" />
          <field name="Transition time" type="uint16" />
          <field name="OptionsMask" type="map8" />
          <field name="OptionsOverride" type="map8" />
        </fields>
      </command>
      <command id="01" name="Move " required="true">
        <fields>
          <field name="Move mode" type="enum8" />
          <field name="Rate" type="uint8" />
          <field name="OptionsMask" type="map8" />
          <field name="OptionsOverride" type="map8" />
        </fields>
      </command>
      <command id="02" name="Step " required="true">
        <fields>
          <field name="Step mode" type="enum8" />
          <field name="Step size" type="uint8" />
          <field name="Transition time" type="uint16" />
          <field name="OptionsMask" type="map8" />
          <field name="OptionsOverride" type="map8" />
        </fields>
      </command>
      <command id="03" name="Stop " required="true">
        <fields>
          <field name="OptionsMask" type="map8" />
          <field name="OptionsOverride" type="map8" />
        </fields>
      </command>
      <command id="04" name="Move to Level (with On/Off) " required="true" />
      <command id="05" name="Move (with On/Off) " required="true" />
      <command id="06" name="Step (with On/Off) " required="true" />
      <command id="07" name="Stop " required="true">
        <fields>
          <field name="OptionsMask" type="map8" />
          <field name="OptionsOverride" type="map8" />
        </fields>
      </command>
      <command id="08" name="Move to Closest Frequency" required="true">
        <fields>
          <field name="Frequency" type="uint16" />
        </fields>
      </command>
    </commands>
  </server>
</ZclCluster>
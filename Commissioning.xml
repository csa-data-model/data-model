<?xml version="1.0"?>
<zcl:cluster xmlns:schemaLocation="http://zigbee.org/zcl/clusters cluster.xsd http://zigbee.org/zcl/types type.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:type="http://zigbee.org/zcl/types" xmlns:xi="http://www.w3.org/2001/XInclude" id="0015" revision="0" name="Commissioning" xmlns:zcl="http://zigbee.org/zcl/clusters">
  <classification role="utility" picsCode="CS" />
  <server>
    <attributes>
      <attribute id="0000" name="ShortAddress" type="uint16" required="true" max="65527" default="RW" />
      <attribute id="0001" name="ExtendedPANId" type="EUI64" required="true" default="RW" />
      <attribute id="0002" name="PANId" type="uint16" required="true" max="65535" default="RW" />
      <attribute id="0003" name="Channelmask" type="map32" required="true" default="RW" />
      <attribute id="0004" name="ProtocolVersion" type="uint8" required="true" min="2" default="RW" />
      <attribute id="0005" name="StackProfile" type="uint8" required="true" min="1" max="2" default="RW" />
      <attribute id="0006" name="StartupControl" type="enum8" required="true" default="RW">
        <restriction>
          <type:enumeration value="00" name="IndicatesThatTheDeviceShouldConsiderItselfPartOfTheNetworkIndicatedByTheExtendedPANIdAttribute.InThisCaseItWillNotPerformAnyExplicitJoinOrRejoinOperation." />
          <type:enumeration value="01" name="IndicatesThatTheDeviceShouldFormANetworkWithExtendedPANIDGivenByTheExtendedPANIdAttribute.TheAIBAttributeApsDesignatedCoordinator(see[B1])ShallBeSetToTRUEInThisCase." />
          <type:enumeration value="02" name="IndicatesThatTheDeviceShouldRejoinTheNetworkWithExtendedPANIDGivenByTheExtendedPANIdAttribute.TheAIBAttributeApsDesignatedCoordinator(see[B1])ShallBeSetToFALSEInThisCase." />
          <type:enumeration value="03" name="IndicatesThatTheDeviceShouldStart“fromScratch”AndJoinTheNetworkUsing(unsecured)MACAssociation.TheAIBAttributeApsDesignatedCoordinator(see[B1])ShallBeSetToFALSEInThisCase." />
        </restriction>
      </attribute>
      <attribute id="0010" name="TrustCenterAddress" type="EUI64" required="true" default="RW" />
      <attribute id="0011" name="TrustCenterMasterKey" type="key128" default="RW" />
      <attribute id="0012" name="NetworkKey" type="key128" required="true" default="RW" />
      <attribute id="0013" name="UseInsecureJoin" type="bool" required="true" default="RW" />
      <attribute id="0014" name="PreconfiguredLinkKey" type="key128" required="true" default="RW" />
      <attribute id="0015" name="NetworkKeySeqNum" type="uint8" required="true" max="255" default="RW" />
      <attribute id="0016" name="NetworkKeyType" type="enum8" required="true" default="RW" />
      <attribute id="0017" name="NetworkManagerAddress" type="uint16" required="true" default="RW" />
    </attributes>
    <commands>
      <command id="00" name="RestartDevice" required="true">
        <fields>
          <field name="Options" type="8-bit bitmap" />
          <field name="Delay" type="Unsigned 8-bit integer" />
          <field name="Jitter" type="Unsigned 8-bit integer" />
        </fields>
      </command>
      <command id="01" name="SaveStartupParameters" />
      <command id="02" name="RestoreStartupParameters" />
      <command id="03" name="ResetStartupParameters" required="true" />
    </commands>
  </server>
  <client>
    <commands>
      <command id="00" name="RestartDeviceResponse" required="true" />
      <command id="01" name="SaveStartupParametersResponse" required="true" />
      <command id="02" name="RestoreStartupParametersResponse" required="true" />
      <command id="03" name="ResetStartupParametersResponse" required="true" />
    </commands>
  </client>
</zcl:cluster>
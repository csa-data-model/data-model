<?xml version="1.0"?>
<zcl:cluster xmlns:schemaLocation="http://zigbee.org/zcl/clusters cluster.xsd http://zigbee.org/zcl/types type.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:type="http://zigbee.org/zcl/types" xmlns:xi="http://www.w3.org/2001/XInclude" id="0202" revision="0" name="Fan Control" xmlns:zcl="http://zigbee.org/zcl/clusters">
  <classification role="application" picsCode="FAN" />
  <server>
    <attributes>
      <attribute id="0000" name="FanMode" type="enum8" writable="true" required="true" default="0x05 (auto)">
        <restriction>
          <type:enumeration value="0" name="Off" />
          <type:enumeration value="1" name="Low" />
          <type:enumeration value="2" name="Medium" />
          <type:enumeration value="3" name="High" />
          <type:enumeration value="4" name="On " />
          <type:enumeration value="5" name="Auto (the fan speed is self-regulated)" />
          <type:enumeration value="6" name="Smart (when the heated/cooled space is occupied, the fan is always on)" />
        </restriction>
      </attribute>
      <attribute id="0001" name="FanModeSequence" type="enum8" writable="true" required="true" default="2" />
    </attributes>
  </server>
</zcl:cluster>